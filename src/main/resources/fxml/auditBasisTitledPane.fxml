<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>


<fx:root type="javafx.scene.control.TitledPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" text="Основание">
    <font>
        <Font size="18.0" />
    </font>
    <content>
        <ScrollPane fitToWidth="true" prefHeight="200.0" prefWidth="200.0">
            <content>
                <GridPane hgap="15.0" vgap="15.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="500.0" minWidth="10.0" prefWidth="250.0" />
                        <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <padding>
                        <Insets bottom="10.0" left="4.0" right="4.0" top="10.0" />
                    </padding>
                    <children>
                        <Label text="Тип основания:" GridPane.rowIndex="1" />
                        <Label text="Наименование основания:" />
                        <Label text="Номер основания:" GridPane.rowIndex="2" />
                        <Label text="Дата основания:" GridPane.rowIndex="3" />
                        <TextField editable="false" GridPane.columnIndex="1" />
                        <TextField editable="false" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <TextField editable="false" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        <TextField editable="false" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                    </children>
                </GridPane>
            </content>
        </ScrollPane>
    </content>

</fx:root>